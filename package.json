{
	"name": "svelte-awesome-slider",
	"version": "1.1.2",
	"description": "A highly customizable slider component library",
	"homepage": "https://svelte-awesome-slider.vercel.app",
	"repository": {
		"type": "git",
		"url": "https://github.com/Ennoriel/svelte-awesome-slider"
	},
	"author": "Maxime Dupont <m.dupont103@gmail.com>",
	"license": "MIT",
	"scripts": {
		"dev": "vite dev --open --host",
		"prebuild": "node scripts/index.js && pnpm format",
		"build": "vite build",
		"preview": "vite preview",
		"package": "svelte-kit sync && svelte-package && publint",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"lint": "eslint .",
		"format": "prettier --write ."
	},
	"files": [
		"dist",
		"!dist/**/*.test.*",
		"!dist/**/*.spec.*"
	],
	"sideEffects": [
		"**/*.css"
	],
	"svelte": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"type": "module",
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"svelte": "./dist/index.js"
		}
	},
	"peerDependencies": {
		"svelte": "^5.0.0"
	},
	"devDependencies": {
		"@changesets/cli": "^2.27.11",
		"@eslint/compat": "^1.2.5",
		"@sveltejs/adapter-vercel": "^5.5.3",
		"@sveltejs/kit": "^2.16.1",
		"@sveltejs/package": "^2.3.9",
		"@sveltejs/vite-plugin-svelte": "^5.0.3",
		"eslint": "^9.18.0",
		"eslint-config-prettier": "^10.0.1",
		"eslint-plugin-svelte": "^2.46.1",
		"globals": "^15.14.0",
		"mdsvex": "^0.12.3",
		"prettier": "^3.4.2",
		"prettier-plugin-svelte": "^3.3.3",
		"publint": "^0.3.2",
		"rehype-autolink-headings": "^7.1.0",
		"rehype-slug": "^6.0.0",
		"svelte": "^5.19.2",
		"svelte-check": "^4.1.4",
		"typescript": "^5.7.3",
		"typescript-eslint": "^8.21.0",
		"vite": "^6.0.11"
	},
	"keywords": [
		"svelte",
		"slider"
	]
}
